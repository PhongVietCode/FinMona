{
  "expo": {
    "name": "FinMona",
    "slug": "expo-redux-template",
    "version": "2.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/87976efc-a8d9-410c-b37b-954f050af63a"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hpcc.expo-redux-template"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.hpcc.expo_redux_template"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      [
        "@sentry/react-native/expo",
        {
          "organization": "phong-company",
          "project": "finmona"
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "extra": {
      "eas": {
        "projectId": "87976efc-a8d9-410c-b37b-954f050af63a"
      }
    }
  }
}
